{
    "name": "zendframework/zend-text",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "text"
    ],
    "homepage": "https://github.com/zendframework/zend-text",
    "autoload": {
        "psr-4": {
            "Zend\\Text\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5",
        "zendframework/zend-stdlib": "dev-develop as 2.8.0",
        "zendframework/zend-servicemanager": "^2.7.3 || ^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev",
            "dev-develop": "2.6-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\Text\\": "test/",
            "ZendTest\\ServiceManager\\": "vendor/zendframework/zend-servicemanager/test"
        }
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "phpunit/PHPUnit": "~4.0",
        "zendframework/zend-config": "~2.5"
    }
}
